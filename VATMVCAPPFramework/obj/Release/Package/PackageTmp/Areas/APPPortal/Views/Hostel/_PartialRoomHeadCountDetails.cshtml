
@model  List<VATMVCAPPFramework.ViewModel.RoomHeadCountviewModel>
@using VATMVCAPPFramework.Utilities
<table class="table convert-data-table table-striped" id="roomheadcountlisttbl">
    <thead>
        <tr>
            <th>S/N</th>
            <th>Hall</th>
            <th>Session</th>
            <th>Room</th>
            <th>Expected</th>
            <th>Actual</th>
            <th>CreatedBy</th>
            <th>Date Created</th>
            <th>Time Created</th>
            <th>Remarks</th>
        </tr>
    </thead>
    <tbody>
        @{int rowNo = 0;}
        @foreach (var item in Model)
        {
            <tr>
                <td>@(rowNo += 1)</td>
                <td>@Html.DisplayFor(modelItem => item.HallName)</td>
                <td>@Html.DisplayFor(modelItem => item.SessionName)</td>
                <td>@Html.DisplayFor(modelItem => item.RoomName)</td>
                <td>@Html.DisplayFor(modelItem => item.NumberOfExpectedOccupants)</td>
                <td>@Html.DisplayFor(modelItem => item.NumberOfActualOccupants)</td>
                <td>@Html.DisplayFor(modelItem => item.CreatedByName)</td>
                <td>@Html.DisplayFor(modelItem => item.DateCreated)</td>
                <td>@Html.DisplayFor(modelItem => item.TimeCreated)</td>
                <td>@Html.DisplayFor(modelItem => item.Remarks)</td>

            </tr>
        }

    </tbody>

</table>

