
@model List<VATMVCAPPFramework.ViewModel.ClubCoordinatorViewModel>
@using VATMVCAPPFramework.Utilities


<table id="clubCoordinatorHistorytbl" class="table convert-data-table table-striped">
    <thead>
        <tr>
            <th>S/N</th>
            <th>Old Coordinator</th>
            <th>New Coordinator</th>
            <th>Effective Date</th>
        </tr>
    </thead>
    <tbody>
        @{int rowNo = 0;}
        @foreach (var item in Model)
        {
            <tr>
                <td>@(rowNo += 1)</td>
                <td>@Html.DisplayFor(modelItem => item.oldCoordinatorName)</td>
                <td>@Html.DisplayFor(modelItem => item.NewCoordinatorName)</td>
                <td>@Html.DisplayFor(modelItem => item.EffectiveDate)</td>
            </tr>
        }

    </tbody>
</table>
