@model  List<VATMVCAPPFramework.ViewModel.BedSpaceViewModel>
<table class="table convert-data-table table-striped" id="lookUptbl">
    <thead>
        <tr>
            <th>S/N</th>

            <th>Hall Name</th>
            <th>Room Name</th>
            <th>Bed Tag</th>
            <th width="5%"></th>
        </tr>
    </thead>
    <tbody>
        @{int rowNo = 0;}
        @foreach (var item in Model)
        {
            <tr>
                <td>@(rowNo += 1)</td>
                <td>@Html.DisplayFor(modelItem => item.HallName)</td>
                <td>@Html.DisplayFor(modelItem => item.RoomName)</td>
                <td>@Html.DisplayFor(modelItem => item.BedTag)</td>
                <td><a data-target='#addedit_modal' role='button' data-edit-id='@Html.DisplayFor(modelItem => item.Id)' class='btn btn-default editBtn' data-toggle='modal'><i class='fa fa-edit'></i>Edit</a></td>

            </tr>
        }

    </tbody>

</table>