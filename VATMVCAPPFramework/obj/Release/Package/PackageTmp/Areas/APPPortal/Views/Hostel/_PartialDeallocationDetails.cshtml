
@model  List<VATMVCAPPFramework.ViewModel.RoomAllocationViewModel>
@using VATMVCAPPFramework.Utilities
<table class="table convert-data-table table-striped" id="lookUptbl">
    <thead>
        <tr>
            <th>S/N</th>

            <th>UserName</th>
            <th>First Name</th>
            <th>LastName</th>
            <th>Mobile Number</th>
            <th>Email Address</th>
            <th>Programme</th>
            <th>Class</th>
            <th>Arm</th>
        </tr>
    </thead>
    <tbody>
        @{int rowNo = 0;}
        @foreach (var item in Model)
        {
            <tr>
                <td>@(rowNo += 1)</td>
                <td>@Html.DisplayFor(modelItem => item.UserName)</td>
                <td>@Html.DisplayFor(modelItem => item.FirstName)</td>
                <td>@Html.DisplayFor(modelItem => item.LastName)</td>
                <td>@Html.DisplayFor(modelItem => item.MobileNumber)</td>
                <td>@Html.DisplayFor(modelItem => item.Email)</td>
                <td>@Html.DisplayFor(modelItem => item.ProgrammeName)</td>
                <td>@Html.DisplayFor(modelItem => item.ClassName)</td>
                <td>@Html.DisplayFor(modelItem => item.ArmName)</td>

            </tr>
        }

    </tbody>

</table>

